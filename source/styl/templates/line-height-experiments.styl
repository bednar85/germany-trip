.grid
    margin: 0 auto
    padding: 1rem 0
    width: 98%

.row
    cf()
    margin-bottom: 1rem

.col
    column(1/5, gutter: 1)
    background: rgba(red, 25%)

    // &:before,
    // span
    //     display: block
    //     font-size: 1rem

    .return-value
        font-size: 1rem
        
    p
        background: rgba(red, 25%)
        line-height: inherit

.fs1
    font-size: 1rem

.fs2
    font-size: 1.25rem

.fs3
    font-size: 1.5rem

.fs4
    font-size: 1.75rem



.fw1
    font-weight: 300

.fw2
    font-weight: 400

.fw3
    font-weight: 700

.fw4
